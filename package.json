{
  "name": "path-minifier",
  "description": "ðŸ”¥ Minify the path list while preserving semantics",
  "version": "2.0.0",
  "main": "dist/index.js",
  "type": "module",
  "license": "MIT",
  "author": {
    "name": "jill64",
    "email": "contact@jill64.dev",
    "url": "https://github.com/jill64",
    "image": "https://avatars.githubusercontent.com/u/143883742?v=4"
  },
  "bugs": "https://github.com/jill64/path-minifier/issues",
  "homepage": "https://github.com/jill64/path-minifier#readme",
  "files": [
    "dist",
    "!**/*.test.*"
  ],
  "exports": {
    "./types": {
      "types": "./dist/index.d.ts",
      "svelte": "./dist/index.js",
      "default": "./dist/index.js"
    },
    ".": {
      "types": "./dist/index.d.ts",
      "svelte": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jill64/path-minifier.git",
    "image": "https://opengraph.githubassets.com/a765b1e7ea974dd66bb5825e2b264db985700bd7ff25a7266132cef3830ea809/jill64/path-minifier"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "application",
    "library",
    "list",
    "llm",
    "minify",
    "path",
    "token",
    "tree"
  ],
  "scripts": {
    "dev": "vite dev",
    "build": "vite build && npm run package",
    "preview": "npm run build && vite preview",
    "prepack": "npm run build",
    "package": "svelte-kit sync && npx @sveltejs/package && npx publint",
    "check": "svelte-kit sync && npx svelte-check",
    "lint": "npm run check && npx eslint .",
    "format": "npx @jill64/psx",
    "test": "vitest",
    "test:e2e": "playwright test"
  },
  "prettier": "@jill64/prettier-config",
  "devDependencies": {
    "@jill64/cf-tiktoken": "1.0.7",
    "@jill64/eslint-config-svelte": "2.0.4",
    "@jill64/playwright-config": "2.4.2",
    "@jill64/prettier-config": "1.0.0",
    "@jill64/sentry-sveltekit-cloudflare": "2.0.5",
    "@jill64/svelte-suite": "2.0.9",
    "@jill64/svelte-tailwind": "1.2.77",
    "@playwright/test": "1.49.1",
    "@sveltejs/adapter-cloudflare": "5.0.1",
    "@sveltejs/kit": "2.16.1",
    "svelte": "5.19.0",
    "typescript": "5.7.3",
    "vite": "6.0.11",
    "vitest": "3.0.3",
    "@sveltejs/vite-plugin-svelte": "5.0.3"
  }
}
