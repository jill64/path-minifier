{
  "name": "path-minifier",
  "description": "ðŸ”¥ Minify the path list while preserving semantics",
  "version": "1.0.30",
  "main": "dist/index.js",
  "type": "module",
  "license": "MIT",
  "author": {
    "name": "jill64",
    "email": "contact@jill64.dev",
    "url": "https://github.com/jill64",
    "image": "https://avatars.githubusercontent.com/u/143883742?v=4"
  },
  "bugs": "https://github.com/jill64/path-minifier/issues",
  "homepage": "https://github.com/jill64/path-minifier#readme",
  "files": [
    "dist",
    "!**/*.test.*"
  ],
  "exports": {
    "./types": {
      "types": "./dist/index.d.ts",
      "svelte": "./dist/index.js",
      "default": "./dist/index.js"
    },
    ".": {
      "types": "./dist/index.d.ts",
      "svelte": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jill64/path-minifier.git",
    "image": "https://opengraph.githubassets.com/7adc9c7ca7d86fb8c40532248f6a9880b1578a3ae54cd7577699d6a0dc0c8df1/jill64/path-minifier"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "list",
    "llm",
    "minify",
    "path",
    "token",
    "tree"
  ],
  "scripts": {
    "dev": "vite dev",
    "build": "vite build && npm run package",
    "preview": "npm run build && vite preview",
    "prepack": "npm run build",
    "package": "svelte-kit sync && npx @sveltejs/package && npx publint",
    "check": "svelte-kit sync && npx svelte-check",
    "lint": "npm run check && npx eslint .",
    "format": "npx @jill64/psx",
    "test": "vitest",
    "test:e2e": "playwright test"
  },
  "prettier": "@jill64/prettier-config",
  "devDependencies": {
    "@jill64/cf-tiktoken": "1.0.6",
    "@jill64/eslint-config-svelte": "1.3.21",
    "@jill64/playwright-config": "2.4.1",
    "@jill64/prettier-config": "1.0.0",
    "@jill64/sentry-sveltekit-cloudflare": "1.7.16",
    "@jill64/svelte-suite": "1.2.246",
    "@jill64/svelte-tailwind": "1.2.64",
    "@playwright/test": "1.48.1",
    "@sveltejs/adapter-cloudflare": "4.7.3",
    "@sveltejs/kit": "2.6.1",
    "svelte": "5.0.0",
    "typescript": "5.6.2",
    "vite": "5.4.8",
    "vitest": "2.1.3",
    "@sveltejs/vite-plugin-svelte": "3.1.2"
  }
}
