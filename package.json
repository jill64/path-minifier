{
  "name": "path-minifier",
  "description": "ðŸ”¥ Minify the path list while preserving semantics",
  "version": "1.0.20",
  "main": "dist/index.js",
  "type": "module",
  "license": "MIT",
  "author": {
    "name": "jill64",
    "email": "contact@jill64.dev",
    "url": "https://github.com/jill64",
    "image": "https://avatars.githubusercontent.com/u/143883742?v=4"
  },
  "bugs": "https://github.com/jill64/path-minifier/issues",
  "homepage": "https://github.com/jill64/path-minifier#readme",
  "files": [
    "dist",
    "!**/*.test.*"
  ],
  "exports": {
    "./types": {
      "types": "./dist/index.d.ts",
      "svelte": "./dist/index.js",
      "default": "./dist/index.js"
    },
    ".": {
      "types": "./dist/index.d.ts",
      "svelte": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jill64/path-minifier.git",
    "image": "https://opengraph.githubassets.com/980a3fdb52a1528e8a9ef088868ef3e653e0b18e418e4493c9b50c61e91850e6/jill64/path-minifier"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "list",
    "llm",
    "minify",
    "path",
    "token",
    "tree"
  ],
  "scripts": {
    "dev": "vite dev",
    "build": "vite build && npm run package",
    "preview": "npm run build && vite preview",
    "prepack": "npm run build",
    "package": "svelte-kit sync && npx @sveltejs/package && npx publint",
    "check": "svelte-kit sync && npx svelte-check",
    "lint": "npm run check && npx eslint .",
    "format": "npx @jill64/psx",
    "test": "vitest",
    "test:e2e": "playwright test"
  },
  "prettier": "@jill64/prettier-config",
  "devDependencies": {
    "@jill64/cf-tiktoken": "1.0.4",
    "@jill64/eslint-config-svelte": "1.2.28",
    "@jill64/playwright-config": "2.3.0",
    "@jill64/prettier-config": "1.0.0",
    "@jill64/sentry-sveltekit-cloudflare": "1.7.13",
    "@jill64/svelte-suite": "1.2.155",
    "@jill64/svelte-tailwind": "1.2.33",
    "@playwright/test": "1.44.1",
    "@sveltejs/adapter-cloudflare": "4.4.0",
    "@sveltejs/kit": "2.5.10",
    "svelte": "4.2.17",
    "typescript": "5.4.5",
    "vite": "5.2.12",
    "vitest": "1.6.0",
    "@sveltejs/vite-plugin-svelte": "3.1.1"
  }
}
