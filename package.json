{
  "name": "path-minifier",
  "description": "ðŸ”¥ Minify the path list while preserving semantics",
  "version": "1.0.0",
  "main": "dist/index.js",
  "type": "module",
  "license": "MIT",
  "author": "jill64 <intents.turrets0h@icloud.com> (https://github.com/jill64)",
  "bugs": "https://github.com/jill64/path-minifier/issues",
  "homepage": "https://github.com/jill64/path-minifier#readme",
  "files": [
    "dist",
    "!**/*.test.*"
  ],
  "exports": {
    "./types": {
      "types": "./dist/index.d.ts",
      "svelte": "./dist/index.js",
      "default": "./dist/index.js"
    },
    ".": {
      "types": "./dist/index.d.ts",
      "svelte": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jill64/path-minifier.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "list",
    "llm",
    "minify",
    "path",
    "token",
    "tree"
  ],
  "scripts": {
    "dev": "vite dev",
    "build": "vite build && npm run package",
    "preview": "npm run build && vite preview",
    "prepack": "npm run build",
    "package": "svelte-kit sync && npx @sveltejs/package && npx publint",
    "check": "svelte-kit sync && npx svelte-check",
    "lint": "npm run check && npx eslint .",
    "format": "npx @jill64/psx",
    "test": "vitest",
    "test:e2e": "playwright test"
  },
  "prettier": "@jill64/prettier-config",
  "devDependencies": {
    "@jill64/cf-tiktoken": "1.0.0",
    "@jill64/eslint-config-svelte": "1.0.1",
    "@jill64/playwright-config": "2.1.0",
    "@jill64/prettier-config": "1.0.0",
    "@jill64/sentry-sveltekit-cloudflare": "1.5.0",
    "@jill64/svelte-suite": "1.0.1",
    "@jill64/svelte-tailwind": "1.0.0",
    "@playwright/test": "1.40.1",
    "@sveltejs/adapter-cloudflare": "2.3.4",
    "@sveltejs/kit": "1.27.7",
    "svelte": "4.2.8",
    "typescript": "5.3.3",
    "vite": "5.0.7",
    "vitest": "1.0.2"
  }
}
